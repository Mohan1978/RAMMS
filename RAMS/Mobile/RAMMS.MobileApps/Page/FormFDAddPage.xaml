                                        <Button Text="Cancel" TextColor="White" Command="{Binding CancelAction}" Grid.Row="0" Grid.Column="1" FontSize="18" BackgroundColor="#1D2D50" CornerRadius="5" ></Button>

                                        <Button Text="Save" IsVisible="{Binding CanSave}" TextColor="White" Command="{Binding SaveAction}" FontSize="18" Grid.Row="0" Grid.Column="2" BackgroundColor="#1D2D50" CornerRadius="5"></Button>

                                        <Button Text="Submit" IsVisible="{Binding CanSave}" TextColor="White" Command="{Binding SubmitAction}" FontSize="18" Grid.Row="0" Grid.Column="3" BackgroundColor="#1D2D50" CornerRadius="5"></Button>
                                    </Grid>
                                </StackLayout>
                            </StackLayout>
                        </Frame>
                    </StackLayout>
                </StackLayout>
            </ScrollView>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>
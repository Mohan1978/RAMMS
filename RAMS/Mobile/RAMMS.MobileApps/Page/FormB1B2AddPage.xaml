<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="RAMMS.MobileApps.Page.FormB1B2AddPage"
             
             xmlns:forms="clr-namespace:SignaturePad.Forms;assembly=SignaturePad.Forms"
             xmlns:page="clr-namespace:RAMMS.MobileApps.Page"
             xmlns:local="clr-namespace:RAMMS.MobileApps"
             xmlns:locals="clr-namespace:RAMMS.MobileApps.Controls;assembly=RAMMS.MobileApps">
    
    <ContentPage.Resources>
        <ResourceDictionary>
            <local:NegateBoolConverter x:Key="NegatebooleanConverter" />
            <page:B1B2ImageSrcConverter x:Key="B1B2ImageSrcConverter"/>
        </ResourceDictionary>
    </ContentPage.Resources>
    <ContentPage.Content>
        <StackLayout>

            <Frame Margin="0,0,0,0" HasShadow="True" BorderColor="#e6e6e6">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="45"/>
                    </Grid.RowDefinitions>
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="10*"/>
                        <ColumnDefinition Width="*"/>
                        <ColumnDefinition Width="1*"/>
                    </Grid.ColumnDefinitions>

                    <Image Source="JKRSLogo.png" Grid.Row="0" Grid.Column="0" Margin="0,-10,0,0"/>
                    <Label Text="ROAD ASSET MANAGEMENT AND MAINTENANCE SYSTEM" LineBreakMode="TailTruncation" HorizontalOptions="CenterAndExpand" VerticalOptions="CenterAndExpand" TextColor="Black" FontAttributes="Bold" FontSize="Large" Grid.Row="0" Grid.Column="2"  Margin="0,-10,0,0"/>
                    <Image Source="ECSBLogo.png" Grid.Row="0" Grid.Column="4"  Margin="0,-10,0,0"/>


                </Grid>
            </Frame>
            <StackLayout>
                <Frame>
                    <StackLayout>
                        <Grid>
                            <Grid.RowDefinitions>

                                <RowDefinition Height="Auto"/>

                            </Grid.RowDefinitions>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="2"/>
                                <ColumnDefinition Width="4*"/>
                                <ColumnDefinition Width="2"/>
                            </Grid.ColumnDefinitions>
                            <StackLayout Orientation="Horizontal" Grid.Row="0" Grid.Column="1" HorizontalOptions="Center">
                                <Label   Text="Form B1/B2 No." FontSize="Large"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />

                                <locals:FrameRenders  BackgroundColor="#f2f2f2" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="3"  ColorName="LightGray"    BorderColor="Black" >
                                    <Label   Text="{Binding SelectedRefNo}" FontSize="Large"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />
                                </locals:FrameRenders>
                            </StackLayout>
                        </Grid>
                    </StackLayout>
                </Frame>
            </StackLayout>

            <ScrollView>
                <StackLayout >

                    <Frame  BackgroundColor="White" BorderColor="#bfbfbf" CornerRadius="5"  Margin="5,0,5,0"  >
                        <StackLayout>
                            <Grid RowSpacing="15" ColumnSpacing="9" IsVisible="{Binding FromAdd}" >
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="44" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="44" />
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="44" />

                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="1*" />
                                    <ColumnDefinition Width="1*" />
                                    <ColumnDefinition Width="1*" />
                                    <ColumnDefinition Width="1*" />

                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>


                                <Label Text="Division" Grid.Row="0" Grid.Column="0"  FontSize="Medium"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />

                                <locals:FrameRenders Grid.Row="1"  Grid.Column="0" BackgroundColor="White" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >

                                    <local:ExtendedPicker ItemsSource="{Binding DDDivisionListItems}" ItemDisplayBinding="{Binding Text}" SelectedIndex="{Binding SelectedDivision, Mode=TwoWay}"  IsEnabled="{Binding IsHeaderEnable}" Title="Select Division"  ClassId ="ClassPickGrey"  VerticalOptions="CenterAndExpand" InputTransparent="{Binding NotToEdit}"  HorizontalOptions="FillAndExpand" TextColor="Gray"/>
                                </locals:FrameRenders>

                                <Label Text="RMU" Grid.Row="0" Grid.Column="1"  Grid.ColumnSpan="2" FontSize="Medium"   FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />

                                <locals:FrameRenders Grid.Row="1"  Grid.Column="1" BackgroundColor="White" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >

                                    <local:ExtendedPicker  ItemsSource="{Binding DDRMUListItems}" ItemDisplayBinding="{Binding Text}" SelectedItem="{Binding SelectedRMU, Mode=TwoWay}" IsEnabled="{Binding IsHeaderEnable}" Title="Select RMU" InputTransparent="{Binding NotToEdit}" ClassId ="ClassPickGrey"  VerticalOptions="CenterAndExpand"  HorizontalOptions="FillAndExpand" TextColor="Gray"/>
                                </locals:FrameRenders>


                                <Label Text="Section Code" Grid.Row="0"  Grid.Column="2"  FontSize="Medium"   FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />

                                <locals:FrameRenders Grid.Row="1"   Grid.Column="2" BackgroundColor="White" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >

                                    <local:ExtendedPicker ItemsSource="{Binding DDSectionListItems}" ItemDisplayBinding="{Binding Text}" SelectedItem="{Binding SelectedSectionCode, Mode=TwoWay}" IsEnabled="{Binding IsHeaderEnable}" Title="Select Section Code" InputTransparent="{Binding NotToEdit}" ClassId ="ClassPickGrey"  VerticalOptions="CenterAndExpand"  HorizontalOptions="FillAndExpand" TextColor="Gray"/>
                                </locals:FrameRenders>


                                <Label Text="Section Name" Grid.Row="0" Grid.Column="3"   FontSize="Medium" FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />

                                <locals:FrameRenders Grid.Row="1" HeightRequest="40"    Grid.Column="3" BackgroundColor="#f2f2f2" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                    <local:EntryControl IsEnabled="{Binding IsHeaderEnable}" InputTransparent="True" Text="{Binding SelectedSectionName}" TextColor="Black" ClassId="ClassDetails" FontFamily="{StaticResource ProximaNovaRegularFont}" Keyboard="Default" />
                                </locals:FrameRenders>


                                <Label Text="Road Code" Grid.Row="2" Grid.Column="0"  FontSize="Medium"   FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />


                                <locals:FrameRenders Grid.Row="3"  Grid.Column="0" BackgroundColor="White" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >

                                    <local:ExtendedPicker ItemsSource="{Binding DDRodeCodeListItems}" ItemDisplayBinding="{Binding Text}" SelectedItem="{Binding SelectedRoadCode, Mode=TwoWay}" IsEnabled="{Binding IsHeaderEnable}" Title="Select Road Code"  ClassId ="ClassPickGrey"  VerticalOptions="CenterAndExpand" InputTransparent="{Binding NotToEdit}"  HorizontalOptions="FillAndExpand" TextColor="Gray"/>
                                </locals:FrameRenders>


                                <Label Text="Road Name"  Grid.Row="2" Grid.Column="1"   FontSize="Medium" FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />

                                <locals:FrameRenders Grid.Row="3"  Grid.Column="1" BackgroundColor="#f2f2f2" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                    <local:EntryControl IsEnabled="{Binding IsHeaderEnable}" InputTransparent="True" Text="{Binding SelectedRoadName}" TextColor="Black" ClassId="ClassDetails" FontFamily="{StaticResource ProximaNovaRegularFont}"  Keyboard="Default" />
                                </locals:FrameRenders>



                                <Label Text="Bridge ID" Grid.Row="2" Grid.Column="2"  FontSize="Medium"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />


                                <locals:FrameRenders Grid.Row="3"  Grid.Column="2" BackgroundColor="#f2f2f2" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                    <local:ExtendedPicker ItemsSource="{Binding DDBridgeListItems}" ItemDisplayBinding="{Binding Text}" SelectedIndex="{Binding SelectedBridge, Mode=TwoWay}"  IsEnabled="{Binding IsHeaderEnable}"   Title="Select Bridge Id"   ClassId ="ClassPickGrey"  VerticalOptions="CenterAndExpand"   HorizontalOptions="FillAndExpand" TextColor="Black" />
                                </locals:FrameRenders>

                                <Label Text="Year" Grid.Row="2" Grid.Column="3" Grid.ColumnSpan="2"  FontSize="Medium"   FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />

                                <locals:FrameRenders Grid.Row="3"  Grid.Column="3" BackgroundColor="White" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"  BorderColor="Black" >
                                    <local:ExtendedPicker ItemsSource="{Binding DDYearListItems}" ItemDisplayBinding="{Binding Text}" SelectedIndex="{Binding SelectedYear, Mode=TwoWay}"  IsEnabled="{Binding IsHeaderEnable}" Title="Select Year"  ClassId ="ClassPickGrey"  VerticalOptions="CenterAndExpand"  InputTransparent="{Binding NotToEdit}"  HorizontalOptions="FillAndExpand" TextColor="Gray"/>
                                </locals:FrameRenders>

                                <Label Text="Date of Inspection (DD-MM-YYYY)" Grid.Row="4" Grid.ColumnSpan="2" Grid.Column="0"  FontSize="Medium"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />

                                <locals:FrameRenders Grid.Row="5"   Grid.Column="0" BackgroundColor="#f2f2f2" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                    <local:CustomDatePicker NullText="dd-mm-yyyy" NullableDate="{Binding SelectedDate}" MinimumDate="{Binding MinimumDate}" MaximumDate="{Binding MaximumDate}" FontSize="16" InputTransparent="{Binding NotToEdit}" Format="dd-MM-yyyy" BackgroundColor="White"   VerticalOptions="CenterAndExpand"   HorizontalOptions="FillAndExpand" TextColor="Gray"   />
                                </locals:FrameRenders>


                                <!--<Label Text="Rating Record No."  Grid.Row="4" Grid.Column="1"  FontSize="Medium"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />

                                <locals:FrameRenders Grid.Row="5"  Grid.Column="1" BackgroundColor="White" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                    <local:ExtendedPicker ItemsSource="{Binding DDRatingListItems}" ItemDisplayBinding="{Binding Text}" SelectedIndex="{Binding SelectedYear, Mode=TwoWay}"  IsEnabled="{Binding IsHeaderEnable}" Title="Select Rating"  ClassId ="ClassPickGrey"  VerticalOptions="CenterAndExpand" FontSize="Default" InputTransparent="{Binding NotToEdit}"  HorizontalOptions="FillAndExpand" TextColor="Gray"/>
                                </locals:FrameRenders>-->

                                <Button  BorderColor="#1D2D50" Grid.Row="5" Grid.Column="3" Command="{Binding OKCommand}"  BackgroundColor="#1D2D50" TextColor="White" BorderWidth="2" Padding="0"  CornerRadius="5" Text="Find Details" FontSize="Medium" />

                            </Grid>

                            <Grid RowSpacing="15" ColumnSpacing="9"  IsVisible="{Binding FromAdd, Converter={StaticResource NegatebooleanConverter}}" >
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="Auto" />
                                    <RowDefinition Height="Auto" />

                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="1*" />
                                    <ColumnDefinition Width="1*" />
                                    <ColumnDefinition Width="1*" />
                                    <ColumnDefinition Width="1*" />

                                    <ColumnDefinition Width="Auto" />
                                </Grid.ColumnDefinitions>

                                <Label Text="Bridge ID" Grid.Row="0" Grid.Column="0"  FontSize="Medium"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />

                                <locals:FrameRenders Grid.Row="1"  Grid.Column="0" BackgroundColor="#f2f2f2" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                    <local:ExtendedPicker ItemsSource="{Binding DDBridgeListItems}" ItemDisplayBinding="{Binding Text}" SelectedIndex="{Binding SelectedBridge, Mode=TwoWay}"  IsEnabled="{Binding IsHeaderEnable}"   Title="Select Bridge"   ClassId ="ClassPickGrey"  VerticalOptions="CenterAndExpand"  HorizontalOptions="FillAndExpand" TextColor="Black" />
                                </locals:FrameRenders>

                                <Label Text="Year" Grid.Row="0" Grid.Column="1" Grid.ColumnSpan="2"  FontSize="Medium"   FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />

                                <locals:FrameRenders Grid.Row="1"  Grid.Column="1" BackgroundColor="White" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"  BorderColor="Black" >
                                    <local:ExtendedPicker ItemsSource="{Binding DDYearListItems}" ItemDisplayBinding="{Binding Text}" SelectedIndex="{Binding SelectedYear, Mode=TwoWay}"  IsEnabled="{Binding IsHeaderEnable}" Title="dd-mm-yyyy"  ClassId ="ClassPickGrey"  VerticalOptions="CenterAndExpand"  InputTransparent="{Binding NotToEdit}"  HorizontalOptions="FillAndExpand" TextColor="Gray"/>
                                </locals:FrameRenders>

                                <Label Text="Date of Inspection (DD-MM-YYYY)" Grid.Row="0" Grid.Column="2"  FontSize="Medium"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />

                                <locals:FrameRenders Grid.Row="1"  Grid.Column="2" BackgroundColor="#f2f2f2" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                    <local:CustomDatePicker IsEnabled="{Binding IsView, Converter={StaticResource NegatebooleanConverter}}" NullText="dd-mm-yyyy" MinimumDate="{Binding MinimumDate}" MaximumDate="{Binding MaximumDate}" NullableDate="{Binding SelectedDate}" InputTransparent="{Binding NotToEdit}" Format="dd-MM-yyyy" BackgroundColor="White"   VerticalOptions="CenterAndExpand"  HorizontalOptions="FillAndExpand" TextColor="Gray"   />
                                </locals:FrameRenders>


                                <!--<Label Text="Rating Record No."  Grid.Row="0" Grid.Column="3"  FontSize="Medium"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />

                                <locals:FrameRenders Grid.Row="1"  Grid.Column="3" BackgroundColor="White" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                    <local:ExtendedPicker ItemsSource="{Binding DDRatingListItems}" ItemDisplayBinding="{Binding Text}" SelectedIndex="{Binding SelectedYear, Mode=TwoWay}" Title="Select Rating"  ClassId ="ClassPickGrey"  VerticalOptions="CenterAndExpand" FontSize="Default" InputTransparent="{Binding NotToEdit}"  HorizontalOptions="FillAndExpand" TextColor="Gray"/>
                                </locals:FrameRenders>-->

                            </Grid>
                        </StackLayout>
                    </Frame>



                    <Frame  >
                        <StackLayout Margin="0,-15,0,0">
                            <!--<Frame CornerRadius="30">-->
                            <Grid>
                                <Grid.RowDefinitions>

                                    <RowDefinition Height="50"/>

                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="*"/>
                                    <ColumnDefinition Width="*"/>
                                </Grid.ColumnDefinitions>
                                <Button  x:Name="btnaddform" BorderColor="#1D2D50" BorderWidth="2"  Text="Form B1" FontSize="Medium" CornerRadius="10"  Command="{Binding AddButtonCommand}" Margin="15,0,15,0" Grid.Row="0" HeightRequest="175"   Grid.Column="0" >
                                    <Button.Triggers>
                                        <DataTrigger TargetType="Button"
                                                 Binding="{Binding IsPhotoTabVisible}" Value="False">
                                            <Setter Property="TextColor" Value="White" />
                                            <Setter Property="BackgroundColor" Value="#1D2D50" />
                                        </DataTrigger>
                                        <DataTrigger TargetType="Button"
                                                 Binding="{Binding IsPhotoTabVisible}" Value="True">
                                            <Setter Property="TextColor" Value="#1D2D50" />
                                            <Setter Property="BackgroundColor" Value="White" />
                                        </DataTrigger>
                                    </Button.Triggers>
                                </Button>
                                <Button  x:Name="btnPhoto" Text="Form B2" BorderWidth="2"  BorderColor="#1D2D50" CornerRadius="10" Command="{Binding PhotoButtonCommand}" Grid.Row="0" Margin="15,0,15,0" HeightRequest="175" FontSize="Medium" Grid.Column="1" >
                                    <Button.Triggers>
                                        <DataTrigger TargetType="Button"
                                                 Binding="{Binding IsPhotoTabVisible}" Value="True">
                                            <Setter Property="TextColor" Value="White" />
                                            <Setter Property="BackgroundColor" Value="#1D2D50" />
                                        </DataTrigger>
                                        <DataTrigger TargetType="Button"
                                                 Binding="{Binding IsPhotoTabVisible}" Value="False">
                                            <Setter Property="TextColor" Value="#1D2D50" />
                                            <Setter Property="BackgroundColor" Value="White" />
                                        </DataTrigger>
                                    </Button.Triggers>
                                </Button>
                            </Grid>
                            <!--</Frame>-->
                        </StackLayout>
                    </Frame>


                    <StackLayout x:Name="stack1" Margin="0,-30,0,0" IsVisible="{Binding IsPhotoTabVisible, Converter={StaticResource NegatebooleanConverter}}">
                        <StackLayout>
                            <Frame BorderColor="Gray" CornerRadius="12" BackgroundColor="White" HasShadow="True" Margin="10">
                                <StackLayout>

                                    <StackLayout >
                                        <Frame BackgroundColor="#f2f2f2" HasShadow="True" BorderColor="#e6e6e6">
                                            <StackLayout Orientation="Horizontal">
                                                <Image x:Name="formToggleImage" Source="RoundedAddIcon.png" HeightRequest="35" WidthRequest="35" VerticalOptions="Start" HorizontalOptions="Start">
                                                </Image>
                                                <Label Text="Asset Identification" Margin="10,0,0,0" FontSize="Large" TextColor="Black" HorizontalOptions="StartAndExpand" VerticalOptions="StartAndExpand">

                                                </Label>
                                            </StackLayout>
                                            <Frame.GestureRecognizers>
                                                <TapGestureRecognizer Command="{Binding ToggleCommand}" CommandParameter="{x:Reference FormDLayout}"/>
                                                <TapGestureRecognizer Command="{Binding ToggleCommand}" CommandParameter="{x:Reference formToggleImage}"/>
                                            </Frame.GestureRecognizers>
                                        </Frame>

                                        <Frame x:Name="FormDLayout"  Margin="0,0,0,0" BackgroundColor="White"   HasShadow="True" BorderColor="#bfbfbf"  IsVisible="False" VerticalOptions="FillAndExpand" HorizontalOptions="FillAndExpand">
                                            <StackLayout>
                                                <Grid RowSpacing="15"  ColumnSpacing="15" >
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="Auto" />
                                                        <RowDefinition Height="Auto" />
                                                        <RowDefinition Height="Auto" />
                                                        <RowDefinition Height="Auto" />

                                                    </Grid.RowDefinitions>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="Auto" />
                                                        <ColumnDefinition Width="1*" />
                                                        <ColumnDefinition Width="1*" />
                                                        <ColumnDefinition Width="1*" />
                                                        <ColumnDefinition Width="1*" />

                                                        <ColumnDefinition Width="Auto" />
                                                    </Grid.ColumnDefinitions>
                                                    <Label Text="Bridge ID" Grid.Row="0" Grid.Column="1"   FontSize="Medium"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="Black" />

                                                    <locals:FrameRenders Grid.Row="1"  Grid.Column="1" Grid.ColumnSpan="2" HeightRequest="40" BackgroundColor="#f2f2f2" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                                        <local:EntryControl   Text="{Binding AiAssetId}" TextColor="Black" ClassId="ClassDetails" FontFamily="{StaticResource ProximaNovaRegularFont}" Keyboard="Default" IsEnabled="False"  />
                                                    </locals:FrameRenders>


                                                    <Label Text="Division" Grid.Row="2" Grid.Column="1"    FontSize="Medium" FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="Black" />

                                                    <locals:FrameRenders Grid.Row="3"  Grid.Column="1" HeightRequest="40" BackgroundColor="#f2f2f2" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                                        <local:EntryControl    Text="{Binding AiDivCode}" TextColor="Black" ClassId="ClassDetails" FontFamily="{StaticResource ProximaNovaRegularFont}"  Keyboard="Default" IsEnabled="False" />
                                                    </locals:FrameRenders>


                                                    <Label Text="RMU" Grid.Row="2" Grid.Column="2"    FontSize="Medium" FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="Black" />

                                                    <locals:FrameRenders Grid.Row="3"  Grid.Column="2" HeightRequest="40" BackgroundColor="#f2f2f2" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                                        <local:EntryControl    Text="{Binding AiRmuName}" TextColor="Black" ClassId="ClassDetails" FontFamily="{StaticResource ProximaNovaRegularFont}"  Keyboard="Default"  IsEnabled="False" />
                                                    </locals:FrameRenders>



                                                    <Label Text="Road Code" Grid.Row="2" Grid.Column="3"    FontSize="Medium" FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="Black" />

                                                    <locals:FrameRenders Grid.Row="3" Grid.Column="3" HeightRequest="40"  BackgroundColor="#f2f2f2" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                                        <local:EntryControl    Text="{Binding AiRdCode}" TextColor="Black" ClassId="ClassDetails" FontFamily="{StaticResource ProximaNovaRegularFont}"  Keyboard="Default"  IsEnabled="False" />
                                                    </locals:FrameRenders>


                                                    <Label Text="Road Name" Grid.Row="2" Grid.Column="4"    FontSize="Medium" FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="Black" />

                                                    <locals:FrameRenders Grid.Row="3" Grid.Column="4"  BackgroundColor="#f2f2f2" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                                        <local:EntryControl   Text="{Binding AiRdName}" TextColor="Black" ClassId="ClassDetails" FontFamily="{StaticResource ProximaNovaRegularFont}"  Keyboard="Default"  IsEnabled="False" />
                                                    </locals:FrameRenders>

                                                </Grid>

                                                <Grid RowSpacing="15"  ColumnSpacing="15" >
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="Auto" />
                                                        <RowDefinition Height="Auto" />


                                                    </Grid.RowDefinitions>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="Auto" />
                                                        <ColumnDefinition Width="1*" />
                                                        <ColumnDefinition Width="0.5*" />
                                                        <ColumnDefinition Width="0.5*" />
                                                        <ColumnDefinition Width="1*" />
                                                        <ColumnDefinition Width="1*" />

                                                        <ColumnDefinition Width="Auto" />
                                                    </Grid.ColumnDefinitions>



                                                    <Label Text="Structure Code" Grid.Row="0" Grid.Column="1"    FontSize="Medium" FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="Black" />

                                                    <locals:FrameRenders Grid.Row="1"  Grid.Column="1" HeightRequest="40" BackgroundColor="#f2f2f2" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                                        <local:EntryControl    Text="{Binding AiStrucCode}" TextColor="Black" ClassId="ClassDetails" FontFamily="{StaticResource ProximaNovaRegularFont}"  Keyboard="Default" IsEnabled="False" />
                                                    </locals:FrameRenders>


                                                    <Label Text="Location CH" Grid.Row="0" Grid.Column="2" Grid.ColumnSpan="3"    FontSize="Medium" FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="Black" />

                                                    <locals:FrameRenders Grid.Row="1"  Grid.Column="2" HeightRequest="40" BackgroundColor="#f2f2f2" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                                        <local:EntryControl    Text="{Binding AiLocChKm}" TextColor="Black" ClassId="ClassDetails" FontFamily="{StaticResource ProximaNovaRegularFont}"  Keyboard="Default" IsEnabled="False"/>
                                                    </locals:FrameRenders>
                                                    <Label Text="+" Grid.Row="1" Grid.Column="3"   Margin="-14,3,0,0"  FontSize="Large"   TextColor="Black" />

                                                    <locals:FrameRenders Grid.Row="1"  Grid.Column="3" HeightRequest="40" BackgroundColor="#f2f2f2" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                                        <local:EntryControl    Text="{Binding AiLocChM}" TextColor="Black" ClassId="ClassDetails" FontFamily="{StaticResource ProximaNovaRegularFont}" Keyboard="Default" IsEnabled="False"/>
                                                    </locals:FrameRenders>


                                                    <Label Text="River Name" Grid.Row="0" Grid.Column="4"    FontSize="Medium" FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="Black" />

                                                    <locals:FrameRenders Grid.Row="1" Grid.Column="4" HeightRequest="40"  BackgroundColor="#f2f2f2" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                                        <local:EntryControl    Text="{Binding AiRiverName}" TextColor="Black" ClassId="ClassDetails" FontFamily="{StaticResource ProximaNovaRegularFont}"  Keyboard="Default" IsEnabled="False"  />
                                                    </locals:FrameRenders>


                                                    <Label Text="Lane Width (m)" Grid.Row="0" Grid.Column="5"    FontSize="Medium" FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="Black" />

                                                    <locals:FrameRenders Grid.Row="1" Grid.Column="5"  BackgroundColor="#f2f2f2" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                                        <local:EntryControl   Text="{Binding AiWidthLane}" TextColor="Black" ClassId="ClassDetails" FontFamily="{StaticResource ProximaNovaRegularFont}"  Keyboard="Default" IsEnabled="False"  />
                                                    </locals:FrameRenders>

                                                </Grid>




                                                <Grid RowSpacing="15"  ColumnSpacing="15" >
                                                    <Grid.RowDefinitions>
                                                        <RowDefinition Height="Auto" />
                                                        <RowDefinition Height="Auto" />


                                                    </Grid.RowDefinitions>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition Width="Auto" />
                                                        <ColumnDefinition Width="1*" />
                                                        <ColumnDefinition Width="1*" />

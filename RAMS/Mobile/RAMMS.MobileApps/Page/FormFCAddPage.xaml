                                    <Button Command="{Binding OKCommand}" Style="{x:StaticResource ButtonStyle}" Margin="60,-5,0,0" Grid.Row="3" CornerRadius="11" FontSize="Small" Grid.Column="3" Text="Find Details"/>
                                </Grid>

                                <Grid  IsVisible="{Binding FromAdd1}" >
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="Auto" />
                                        <RowDefinition Height="Auto" />

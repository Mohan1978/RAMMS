                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>
                            <RowDefinition Height="Auto"/>

                        </Grid.RowDefinitions>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                            <ColumnDefinition Width="*"/>
                        </Grid.ColumnDefinitions>


                        <Frame BackgroundColor="White" BorderColor="#bfbfbf" Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3">
                            <StackLayout>
                                <Grid IsVisible="{Binding FromAdd}" Padding="0"  RowSpacing="0" ColumnSpacing="5"   >
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="44"/>
                                        <RowDefinition Height="44"/>
                                        <RowDefinition Height="44"/>
                                        <RowDefinition Height="44"/>
                                    </Grid.RowDefinitions>
                                    <Grid.ColumnDefinitions>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                        <ColumnDefinition Width="*"/>
                                    </Grid.ColumnDefinitions>

                                    <Label VerticalOptions="Center" HorizontalTextAlignment="Start"  Text="Division" Grid.Row="0" Grid.Column="0" FontSize="Medium"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />
                                    <Label VerticalOptions="Center" HorizontalTextAlignment="Start"  Text="RMU"  Grid.Row="0" Grid.Column="1" FontSize="Medium"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />
                                    <local:FrameRenders  BackgroundColor="White" IsClippedToBounds="True" Padding="10,0,10,0" ColorName="LightGray"   FrameCornerRadius="10" BorderColor="Black" Grid.Row="1" Grid.Column="0">
                                        <local1:ExtendedPicker ItemsSource="{Binding DDDivisionListItems}" ItemDisplayBinding="{Binding Text}" SelectedIndex="{Binding SelectedDivision, Mode=TwoWay}"  IsEnabled="{Binding IsHeaderEnable}" CharacterSpacing="20"  Title="Select Division"   ClassId ="ClassPickGrey"  VerticalOptions="CenterAndExpand"  HorizontalOptions="FillAndExpand" TextColor="Black" />
                                    </local:FrameRenders>
                                    <local:FrameRenders  IsClippedToBounds="True" Padding="10,0,10,0" BackgroundColor="White" ColorName="LightGray"   FrameCornerRadius="10" BorderColor="Black" Grid.Row="1" Grid.Column="1">
                                        <local1:ExtendedPicker ItemsSource="{Binding DDRMUListItems}" ItemDisplayBinding="{Binding Text}" SelectedItem="{Binding SelectedRMU, Mode=TwoWay}" IsEnabled="{Binding IsHeaderEnable}" Title="Select RMU"   ClassId ="ClassPickGrey"  VerticalOptions="CenterAndExpand"   HorizontalOptions="FillAndExpand" TextColor="Black" />
                                    </local:FrameRenders>

                                    <Label  VerticalOptions="Center"  HorizontalTextAlignment="Start" Text="Section Code"  Grid.Row="0" Grid.Column="2" FontSize="Medium"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />
                                    <Label  VerticalOptions="Center" HorizontalTextAlignment="Start"  Text="Section Name" Grid.Row="0" Grid.Column="3" FontSize="Medium"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000"/>
                                    <local:FrameRenders Grid.Row="1" Grid.Column="2" IsClippedToBounds="True" Padding="10,0,10,0" BackgroundColor="White" ColorName="LightGray"   FrameCornerRadius="10" BorderColor="Black" >
                                        <local1:ExtendedPicker ItemsSource="{Binding DDSectionListItems}" ItemDisplayBinding="{Binding Text}" SelectedItem="{Binding SelectedSectionCode, Mode=TwoWay}" IsEnabled="{Binding IsHeaderEnable}" Title="Select Section Code"    ClassId ="ClassPickGrey"  VerticalOptions="CenterAndExpand"   HorizontalOptions="FillAndExpand" TextColor="Black" />
                                    </local:FrameRenders>

                                    <local:FrameRenders  IsClippedToBounds="True" Padding="10,0,10,0" BackgroundColor="#f2f2f2" ColorName="LightGray"   FrameCornerRadius="10" BorderColor="Black"  Grid.Row="1" Grid.Column="3">
                                        <local1:EntryControl FontSize="15"  IsEnabled="False" Text="{Binding SelectedSectionName}" FontAttributes="Bold" TextColor="Black" ClassId="ClassDetails" FontFamily="{StaticResource ProximaNovaRegularFont}" Keyboard="Default" />
                                    </local:FrameRenders>

                                    <Label  VerticalOptions="Center" HorizontalTextAlignment="Start" Text="Road Code" Margin="10,0,0,0" Grid.Row="2" Grid.Column="0" FontSize="Medium"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000"/>
                                    <Label  VerticalOptions="Center" HorizontalTextAlignment="Start" Text="Road Name" Margin="10,0,0,0" Grid.Row="2" Grid.Column="1" FontSize="Medium"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000"/>
                                    <Label   VerticalOptions="Center" HorizontalTextAlignment="Start" Text="Year of Inspection (YYYY)"  Margin="10,0,0,0" Grid.Row="2" Grid.Column="2" Grid.ColumnSpan="2" FontSize="Medium"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />


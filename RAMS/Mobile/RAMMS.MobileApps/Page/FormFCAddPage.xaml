                                        <ColumnDefinition Width="Auto" />
                                    </Grid.ColumnDefinitions>

                                    <Label Text="Road Code" VerticalOptions="Center" HorizontalTextAlignment="Start"  Margin="10,0,0,0" Grid.Row="0" Grid.Column="0" FontSize="Medium"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000"/>


                                    <local:FrameRenders Grid.Row="1" Grid.Column="0" IsClippedToBounds="True" Padding="10,0,10,0" BackgroundColor="White" ColorName="LightGray"   FrameCornerRadius="10" BorderColor="Black" >
                                        <local1:ExtendedPicker ItemsSource="{Binding DDRodeCodeListItems}" ItemDisplayBinding="{Binding Text}" SelectedItem="{Binding SelectedRoadCode, Mode=TwoWay}" IsEnabled="{Binding IsHeaderEnable}" Title="Select Road Code"   ClassId ="ClassPickGrey"  VerticalOptions="CenterAndExpand"   HorizontalOptions="FillAndExpand" TextColor="Black" />
                                    </local:FrameRenders>
                                    <Label Text="Road Name"  Grid.Row="0" Grid.Column="1"  FontSize="Medium"  FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />

                                    <local:FrameRenders Grid.Row="1"  Grid.Column="1" BackgroundColor="#f2f2f2" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"    BorderColor="Black" >
                                        <localss:EntryControl FontSize="15" HeightRequest="40" Text="{Binding RoadName}"  IsEnabled="{Binding IsHeaderEnable}"   ClassId ="ClassPickGrey"  VerticalOptions="CenterAndExpand"   HorizontalOptions="FillAndExpand" TextColor="Black" />
                                    </local:FrameRenders>

                                    <Label Text="Year of Inspection (YYYY)"   Grid.Row="0" Grid.Column="2" Grid.ColumnSpan="2" FontSize="Medium"   FontFamily="{StaticResource ProximaNovaRegularFont}" TextColor="#000" />

                                    <local:FrameRenders Grid.Row="1"  Grid.Column="2" BackgroundColor="#f2f2f2" FrameCornerRadius="7"   IsClippedToBounds="True" Padding="0,0,0,0"  ColorName="LightGray"  BorderColor="Black" >
                                        <localss:EntryControl FontSize="15" HeightRequest="40" Text="{Binding YearOfInsp}"  IsEnabled="{Binding IsHeaderEnable}"  ClassId ="ClassPickGrey"  VerticalOptions="CenterAndExpand"  InputTransparent="{Binding NotToEdit}"  HorizontalOptions="FillAndExpand" TextColor="Gray"/>
                                    </local:FrameRenders>

                                </Grid>






                                <Grid  IsVisible="{Binding FromEdit}" >
